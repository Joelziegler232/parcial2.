<p id="notice"><%= notice %></p>

<h1>Medical Center Workers</h1>

<table>
  <thead>
    <tr>
      <th>Medical center</th>
      <th>Worker</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @medical_center_workers.each do |medical_center_worker| %>
      <tr>
        <td><%= medical_center_worker.medical_center_id %></td>
        <td><%= medical_center_worker.worker_id %></td>
        <td><%= link_to 'Show', medical_center_worker %></td>
        <td><%= link_to 'Edit', edit_medical_center_worker_path(medical_center_worker) %></td>
        <td><%= link_to 'Destroy', medical_center_worker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Medical Center Worker', new_medical_center_worker_path %>
